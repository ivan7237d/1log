{
  "name": "@1log/jest",
  "version": "0.0.3",
  "description": "A 1log plugin that buffers log entries and lets you snapshot them in Jest tests.",
  "repository": "https://github.com/ivan7237d/1log.git",
  "license": "MIT",
  "module": "build/module/index.js",
  "main": "build/main/index.js",
  "types": "build/types/index.d.ts",
  "files": [
    "build/module",
    "build/main",
    "build/types",
    "!**/*.test.*",
    "!**/setupTests.*"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "@1log/core": "workspace:^"
  },
  "dependencies": {
    "antiutils": "^4.0.2"
  },
  "devDependencies": {
    "@1log/core": "workspace:^",
    "pretty-format": "^29.3.1"
  }
}
