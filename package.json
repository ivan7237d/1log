{
  "name": "1log",
  "version": "3.0.2",
  "description": "A logging framework",
  "repository": "https://github.com/ivan7237d/1log.git",
  "license": "MIT",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "files": [
    "build/main",
    "build/module",
    "!**/*.test.*",
    "!**/setupTests.*"
  ],
  "sideEffects": false,
  "scripts": {
    "prebuild": "rm -rf build",
    "build": "run-p --print-label build:*",
    "build:main": "tsc",
    "build:module": "tsc -p tsconfig.module.json",
    "test": "run-p --print-label build 'test:!(tsc)'",
    "test:tsc": "tsc --noEmit",
    "test:lint": "eslint --ignore-path .gitignore .",
    "test:prettier": "prettier --ignore-path .gitignore --list-different --ignore-unknown '**' '!pnpm-lock.yaml'",
    "test:jest": "jest",
    "watch:jest": "jest --watch"
  },
  "dependencies": {
    "antiutils": "^4.0.1",
    "monomemo": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-expect-type": "^0.2.1",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3"
  }
}
